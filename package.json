{
  "name": "gruvbox-crisp-tex",
  "publisher": "vatsalsy",
  "displayName": "Gruvbox Crisp Color Theme Collection",
  "description": "A collection of high-contrast themes based on Gruvbox with clear foregrounds",
  "version": "1.1.8",
  "engines": {
    "vscode": "^1.50.0"
  },
  "icon": "icon.png",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "gruvbox",
    "dark theme",
    "high contrast",
    "latex"
  ],
  "galleryBanner": {
    "color": "#1d2021",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "Gruvbox Crisp (High Contrast, with TeX)",
        "uiTheme": "vs-dark",
        "path": "./themes/gruvbox-crisp-high-contrast.json",
        "semanticHighlighting": "configuredByTheme"
      },
      {
        "label": "Gruvbox Crisp (Medium, with TeX)",
        "uiTheme": "vs-dark",
        "path": "./themes/gruvbox-crisp-medium.json",
        "semanticHighlighting": "configuredByTheme"
      },
      {
        "label": "Gruvbox Crisp (Soft, with TeX)",
        "uiTheme": "vs-dark",
        "path": "./themes/gruvbox-crisp-soft.json",
        "semanticHighlighting": "configuredByTheme"
      },
      {
        "label": "Gruvbox Crisp Anysphere Blend",
        "uiTheme": "vs-dark",
        "path": "./themes/gruvbox-crisp-purple.json",
        "semanticHighlighting": "configuredByTheme"
      },
      {
        "label": "Gruvbox Crisp Anysphere Blend (High Contrast)",
        "uiTheme": "vs-dark",
        "path": "./themes/gruvbox-crisp-high-contrast-purple.json",
        "semanticHighlighting": "configuredByTheme"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VatsalSy/gruvbox_custom_themes"
  },
  "bugs": {
    "url": "https://github.com/VatsalSy/gruvbox_custom_themes/issues"
  },
  "author": {
    "name": "Vatsal"
  },
  "license": "MIT",
  "homepage": "https://github.com/VatsalSy/gruvbox_custom_themes#readme",
  "scripts": {
    "build": "node scripts/build-themes.js",
    "prebuild": "echo 'Building theme variants from base configuration...'",
    "postbuild": "echo 'Theme generation complete!'"
  }
}