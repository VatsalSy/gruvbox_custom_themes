{
  "$schema": "vscode://schemas/color-theme",
  "name": "Gruvbox Crisp Anysphere Blend",
  "type": "dark",
  "semanticHighlighting": true,

  "tokenColors": [
    //
    //  NORMAL / GLOBAL
    //
    {
      "settings": {
        "foreground": "#D8DEE9" // normal text - using Cursor's text color
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#88C0D0" // Using Cursor's accent blue
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6d6d6d", // Using Cursor's comment color
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant", "support.constant", "variable.arguments"],
      "settings": {
        "foreground": "#68246D" // Using requested purple
      }
    },
    {
      "scope": "constant.rgb-value",
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "scope": "entity.name.selector",
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#EBCB8B" // Cursor theme yellow
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.tag"],
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#BF616A" // Cursor theme red
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#68246D" // Using requested purple
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "#8ec07c"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#fe8019"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#b8bb26"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#b8bb26"
      }
    },
    {
      "scope": "meta.header.diff",
      "settings": {
        "foreground": "#fe8019"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#BF616A" // Cursor theme red
      }
    },
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#EBCB8B" // Cursor theme yellow
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#EBCB8B" // Cursor theme yellow
      }
    },
    {
      "scope": "string.escape",
      "settings": {
        "foreground": "#BF616A" // Cursor theme red
      }
    },
    {
      "scope": "string.quasi",
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "scope": "string.entity",
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "scope": "object",
      "settings": {
        "foreground": "#D8DEE9" // Cursor theme foreground
      }
    },
    {
      "scope": "module.node",
      "settings": {
        "foreground": "#88C0D0" // Cursor theme blue
      }
    },
    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#88C0D0" // Cursor theme blue
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#BF616A" // Cursor theme red
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#BF616A" // Cursor theme red
      }
    },
    {
      "scope": "keyword.control.module",
      "settings": {
        "foreground": "#8ec07c"
      }
    },
    {
      "scope": "keyword.control.less",
      "settings": {
        "foreground": "#d79921"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#8ec07c"
      }
    },
    {
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#fe8019"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#b8bb26"
      }
    },
    {
      "scope": "metatag.php",
      "settings": {
        "foreground": "#fe8019"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#689d6a"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#b8bb26"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "meta.type.name",
        "meta.return.type",
        "meta.return-type",
        "meta.cast",
        "meta.type.annotation",
        "support.type",
        "storage.type.cs",
        "variable.class"
      ],
      "settings": {
        "foreground": "#EBCB8B" // Cursor theme yellow
      }
    },
    {
      "scope": ["variable.this", "support.variable"],
      "settings": {
        "foreground": "#68246D" // Using requested purple
      }
    },
    {
      "scope": [
        "entity.name",
        "entity.static",
        "entity.name.class.static.function",
        "entity.name.function",
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#EBCB8B" // Cursor theme yellow
      }
    },
    {
      "name": "Function declarations",
      "scope": ["entity.function", "entity.name.function.static"],
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "scope": "entity.name.function.function-call",
      "settings": {
        "foreground": "#88C0D0" // Cursor theme blue
      }
    },
    {
      "scope": "support.function.builtin",
      "settings": {
        "foreground": "#EBCB8B" // Cursor theme yellow
      }
    },
    {
      "scope": [
        "entity.name.method",
        "entity.name.method.function-call",
        "entity.name.static.function-call"
      ],
      "settings": {
        "foreground": "#689d6a"
      }
    },
    {
      "scope": "brace",
      "settings": {
        "foreground": "#d5c4a1"
      }
    },
    {
      "name": "variables",
      "scope": [
        "meta.parameter.type.variable",
        "variable.parameter",
        "variable.name",
        "variable.other",
        "variable",
        "string.constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#88C0D0" // Cursor theme blue
      }
    },
    {
      "scope": "prototype",
      "settings": {
        "foreground": "#68246D" // Using requested purple
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#a89984"
      }
    },
    {
      "scope": "punctuation.quoted",
      "settings": {
        "foreground": "#ebdbb2"
      }
    },
    {
      "scope": "punctuation.quasi",
      "settings": {
        "foreground": "#BF616A" // Cursor theme red
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },

    //
    // LANGUAGE-SPECIFIC SCOPES
    //

    // PYTHON ------------------------------------
    {
      "name": "Python function",
      "scope": ["meta.function.python", "entity.name.function.python"],
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "name": "Python Function/Class definition keywords",
      "scope": [
        "storage.type.function.python",
        "storage.modifier.declaration",
        "storage.type.class.python",
        "storage.type.string.python"
      ],
      "settings": {
        "foreground": "#BF616A" // Cursor theme red
      }
    },
    {
      "name": "Async keyword",
      "scope": ["storage.type.function.async.python"],
      "settings": {
        "foreground": "#BF616A" // Cursor theme red
      }
    },
    {
      "name": "Python Function Call",
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#88C0D0" // Cursor theme blue
      }
    },
    {
      "name": "Python Function Arguments",
      "scope": "meta.function-call.arguments",
      "settings": {
        "foreground": "#D8DEE9" // Cursor theme foreground
      }
    },
    {
      "name": "Python Function decorator",
      "scope": "entity.name.function.decorator",
      "settings": {
        "foreground": "#EBCB8B", // Cursor theme yellow
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python ALL CAPS",
      "scope": "constant.other.caps",
      "settings": {
        "fontStyle": "bold"
      }
    },

    // SHELL --------------------------------------
    {
      "scope": "keyword.operator.logical",
      "settings": {
        "foreground": "#fb4934"
      }
    },
    {
      "scope": "punctuation.definition.logical-expression",
      "settings": {
        "foreground": "#fe8019"
      }
    },
    {
      "scope": [
        "string.interpolated.dollar.shell",
        "string.interpolated.backtick.shell"
      ],
      "settings": {
        "foreground": "#8ec07c"
      }
    },

    // C / C++ -------------------------------------
    {
      "scope": "keyword.control.directive",
      "settings": {
        "foreground": "#8ec07c"
      }
    },
    {
      "scope": "support.function.C99",
      "settings": {
        "foreground": "#fabd2f"
      }
    },

    // C# ------------------------------------------
    {
      "name": "C# functions & namespace",
      "scope": [
        "meta.function.cs",
        "entity.name.function.cs",
        "entity.name.type.namespace.cs"
      ],
      "settings": {
        "foreground": "#b8bb26"
      }
    },
    {
      "name": "C# Variables",
      "scope": [
        "keyword.other.using.cs",
        "entity.name.variable.field.cs",
        "entity.name.variable.local.cs",
        "variable.other.readwrite.cs"
      ],
      "settings": {
        "foreground": "#8ec07c"
      }
    },
    {
      "name": "C# This",
      "scope": ["keyword.other.this.cs", "keyword.other.base.cs"],
      "settings": {
        "foreground": "#d3869b"
      }
    },

    // MAKEFILE ------------------------------------
    {
      "scope": "meta.scope.prerequisites",
      "settings": {
        "foreground": "#fabd2f"
      }
    },
    {
      "scope": "entity.name.function.target",
      "settings": {
        "foreground": "#b8bb26",
        "fontStyle": "bold"
      }
    },

    // JAVA ----------------------------------------
    {
      "name": "Java import/package storage modifiers",
      "scope": [
        "storage.modifier.import.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#bdae93"
      }
    },
    {
      "scope": ["keyword.other.import.java", "keyword.other.package.java"],
      "settings": {
        "foreground": "#8ec07c"
      }
    },
    {
      "scope": "storage.type.java",
      "settings": {
        "foreground": "#fabd2f"
      }
    },
    {
      "scope": "storage.type.annotation",
      "settings": {
        "foreground": "#83a598",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.other.documentation.javadoc",
      "settings": {
        "foreground": "#8ec07c"
      }
    },
    {
      "scope": "comment.block.javadoc variable.parameter.java",
      "settings": {
        "foreground": "#b8bb26",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "source.java variable.other.object",
        "source.java variable.other.definition.java"
      ],
      "settings": {
        "foreground": "#ebdbb2"
      }
    },

    // LISP ----------------------------------------
    {
      "name": "Lisp optional function parameters",
      "scope": "meta.function-parameters.lisp",
      "settings": {
        "foreground": "#fabd2f"
      }
    },

    // MARKUP --------------------------------------
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#928374",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#d3869b"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#fe8019"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#fe8019"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#b8bb26"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#d65d0e"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#fe8019"
      }
    },
    {
      "scope": "markup.punctuation.quote.beginning",
      "settings": {
        "foreground": "#98971a"
      }
    },
    {
      "scope": "markup.punctuation.list.beginning",
      "settings": {
        "foreground": "#83a598"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#8ec07c"
      }
    },

    // JSON ----------------------------------------
    {
      "scope": "string.quoted.double.json",
      "settings": {
        "foreground": "#88C0D0" // Cursor theme blue
      }
    },
    {
      "name": "JSON Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "name": "JSON Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "name": "JSON Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#68246D" // Using requested purple
      }
    },
    {
      "name": "JSON Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },

    // CSS -----------------------------------------
    {
      "scope": "entity.other.attribute-name.css",
      "settings": {
        "foreground": "#EBCB8B" // Cursor theme yellow
      }
    },
    {
      "scope": "source.css meta.selector",
      "settings": {
        "foreground": "#D8DEE9" // Cursor theme foreground
      }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#EBCB8B" // Cursor theme yellow
      }
    },
    {
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "scope": [
        "source.css support.function.transform",
        "source.css support.function.timing-function",
        "source.css support.function.misc"
      ],
      "settings": {
        "foreground": "#BF616A" // Cursor theme red
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.property-value",
        "constant.rgb-value",
        "support.property-value.scss",
        "constant.rgb-value.scss"
      ],
      "settings": {
        "foreground": "#B48EAD" // Cursor theme magenta
      }
    },
    {
      "scope": ["entity.name.tag.css"],
      "settings": {
        "fontStyle": ""
      }
    },

    // HTML / XML ----------------------------------
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#88C0D0" // Cursor theme blue
      }
    },
    {
      "scope": ["text.html entity.name.tag", "text.html punctuation.tag"],
      "settings": {
        "foreground": "#A3BE8C", // Cursor theme green
        "fontStyle": "bold"
      }
    },

    // JAVASCRIPT ----------------------------------
    {
      "scope": ["source.js variable.language"],
      "settings": {
        "foreground": "#EBCB8B" // Cursor theme yellow
      }
    },

    // TYPESCRIPT ----------------------------------
    {
      "scope": ["source.ts variable.language"],
      "settings": {
        "foreground": "#EBCB8B" // Cursor theme yellow
      }
    },

    // GOLANG --------------------------------------
    {
      "scope": ["source.go storage.type"],
      "settings": {
        "foreground": "#fabd2f"
      }
    },
    {
      "scope": ["source.go entity.name.import"],
      "settings": {
        "foreground": "#b8bb26"
      }
    },
    {
      "scope": ["source.go keyword.package", "source.go keyword.import"],
      "settings": {
        "foreground": "#8ec07c"
      }
    },
    {
      "scope": ["source.go keyword.interface", "source.go keyword.struct"],
      "settings": {
        "foreground": "#83a598"
      }
    },
    {
      "scope": ["source.go entity.name.type"],
      "settings": {
        "foreground": "#ebdbb2"
      }
    },
    {
      "scope": ["source.go entity.name.function"],
      "settings": {
        "foreground": "#d3869b"
      }
    },

    // CUCUMBER ------------------------------------
    {
      "scope": ["keyword.control.cucumber.table"],
      "settings": {
        "foreground": "#83a598"
      }
    },

    // REASONML ------------------------------------
    {
      "name": "ReasonML String",
      "scope": ["source.reason string.double", "source.reason string.regexp"],
      "settings": {
        "foreground": "#b8bb26"
      }
    },
    {
      "name": "ReasonML equals sign",
      "scope": ["source.reason keyword.control.less"],
      "settings": {
        "foreground": "#8ec07c"
      }
    },
    {
      "name": "ReasonML variable",
      "scope": ["source.reason entity.name.function"],
      "settings": {
        "foreground": "#83a598"
      }
    },
    {
      "name": "ReasonML property",
      "scope": [
        "source.reason support.property-value",
        "source.reason entity.name.filename"
      ],
      "settings": {
        "foreground": "#fe8019"
      }
    },

    // POWERSHELL ----------------------------------
    {
      "name": "Powershell member",
      "scope": ["source.powershell variable.other.member.powershell"],
      "settings": {
        "foreground": "#fe8019"
      }
    },
    {
      "name": "Powershell function",
      "scope": ["source.powershell support.function.powershell"],
      "settings": {
        "foreground": "#fabd2f"
      }
    },
    {
      "name": "Powershell function attribute",
      "scope": ["source.powershell support.function.attribute.powershell"],
      "settings": {
        "foreground": "#bdae93"
      }
    },
    {
      "name": "Powershell hashtable member",
      "scope": [
        "source.powershell meta.hashtable.assignment.powershell variable.other.readwrite.powershell"
      ],
      "settings": {
        "foreground": "#fe8019"
      }
    },

    //
    // TeX/LaTeX SUPPORT
    //
    {
      "name": "LaTeX - commands",
      "scope": [
        "support.function.beamer.latex",
        "support.function.general.latex",
        "support.function.section.latex",
        "support.function.latex",
        "keyword.control.latex" // \begin, \end, etc.
      ],
      "settings": {
        "foreground": "#EBCB8B" // Cursor theme yellow
      }
    },
    {
      "name": "LaTeX - math blocks ($...$)",
      "scope": [
        "string.other.math.block.latex",
        "string.other.math.latex"
      ],
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "name": "LaTeX - environment (\\begin{...}, \\end{...})",
      "scope": ["meta.environment.latex"],
      "settings": {
        "foreground": "#A3BE8C" // Cursor theme green
      }
    },
    {
      "name": "LaTeX - punctuation/braces",
      "scope": [
        "punctuation.definition.arguments.latex",
        "punctuation.definition.bracket.latex",
        "punctuation.definition.group.latex"
      ],
      "settings": {
        "foreground": "#D8DEE9" // Cursor theme foreground
      }
    },
    {
      "name": "LaTeX - references (\\ref{}, \\label{}, etc.)",
      "scope": ["constant.other.reference.latex"],
      "settings": {
        "foreground": "#68246D" // Using requested purple
      }
    },
    {
      "name": "LaTeX - invalid",
      "scope": ["invalid.illegal.latex"],
      "settings": {
        "foreground": "#BF616A" // Cursor theme red
      }
    },
    {
      "name": "LaTeX - comments (%)",
      "scope": ["comment.line.percentage.latex"],
      "settings": {
        "foreground": "#6d6d6d", // Cursor theme comment color
        "fontStyle": "italic"
      }
    }
  ],

  "colors": {
    // BASE COLORS
    "focusBorder": "#00000000", // From Cursor theme
    "foreground": "#CCCCCCdd", // From Cursor theme
    "widget.shadow": "#00000066", // From Cursor theme
    "selection.background": "#FFFFFF33", // From Cursor theme
    "errorForeground": "#BF616A", // From Cursor theme
    "icon.foreground": "#D8DEE9",

    // BUTTON
    "button.background": "#565656", // From Cursor theme
    "button.foreground": "#ececec", // From Cursor theme
    "button.hoverBackground": "#767676", // From Cursor theme

